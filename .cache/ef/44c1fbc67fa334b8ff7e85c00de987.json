"src/script.js","name":"remove","original":{"line":37,"column":21},"generated":{"line":1,"column":607}},{"source":"src/script.js","original":{"line":37,"column":28},"generated":{"line":1,"column":614}},{"source":"src/script.js","name":"navbar","original":{"line":38,"column":2},"generated":{"line":1,"column":622}},{"source":"src/script.js","name":"classList","original":{"line":38,"column":9},"generated":{"line":1,"column":624}},{"source":"src/script.js","name":"remove","original":{"line":38,"column":19},"generated":{"line":1,"column":634}},{"source":"src/script.js","original":{"line":38,"column":26},"generated":{"line":1,"column":641}}],"sources":{"src/script.js":"// toggle icon navbar\nlet menuIcon = document.querySelector(\"#menu-icon\");\nlet navbar = document.querySelector(\".navbar\");\n\nmenuIcon.onclick = () => {\n  menuIcon.classList.toggle(\"bx-x\");\n  navbar.classList.toggle(\"active\");\n};\n\n// scoll section active\nlet sections = document.querySelectorAll(\"section\");\nlet navLinks = document.querySelectorAll(\"header nav a\");\n\nwindow.onscroll = () => {\n  sections.forEach((sec) => {\n    let top = window.scrollY;\n    let offset = sec.offsetTop - 150;\n    let height = sec.offsetHeight;\n    let id = sec.getAttribute(\"id\");\n\n    if (top >= offset && top < offset + height) {\n      navLinks.forEach((links) => {\n        links.classList.remove(\"active\");\n        document\n          .querySelector(\"header nav a[href*=\" + id + \"]\")\n          .classList.add(\"active\");\n      });\n    }\n  });\n  // scoll section active\n  let header = document.querySelector(\"header\");\n\n  header.classList.toogle(\"sticky\", window.scrollY > 100);\n\n  // remove toggle icon and navbar when click navbar link (scoll)\n\n  menuIcon.classList.remove(\"bx-x\");\n  navbar.classList.remove(\"active\");\n};\n"},"lineCount":null}},"error":null,"hash":"2fc5a8caabd5b57a79ab327e0ba8c3c2","cacheData":{"env":{}}}